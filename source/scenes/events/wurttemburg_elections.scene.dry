title: Württemberg Landtag Election
view-if: year = next_election_year_wurttemberg and month >= next_election_month_wurttemberg
new-page: true
tags: event
priority: 3
on-arrival: next_election_year_wurttemberg = next_election_year_wurttemberg + 4; next_election_time_wurttemberg = time + 48; was_dvp_wurttemberg = 0; was_in_dvp_majority_wurttemberg = 0; was_in_weimar_coalition_wurttemberg = 0; was_in_grand_coalition_wurttemberg = 0; was_in_bourgeois_coalition_wurttemberg = 0; was_in_far_right_coalition_wurttemberg = 0;
set-jump: new_elections

= Württemberg Elections, [+ month : month +] [+ year +]

The Württemberg elections are to be held now.

- @new_elections: May we do our best...

@new_elections
on-arrival: {!
for (var c of Q.classes) {
    var class_votes = 0;
    for (var party of Q.parties) {
        if (Q[c + '_' + party] < 0) {
            Q[c + '_' + party] = 0;
        }
        class_votes += Q[c + '_' + party];
    }
    for (var party of Q.parties) {
        Q[c + '_' + party + '_normalized_wurttemberg'] = Math.round(100 * Q[c + '_' + party] / class_votes);
    }
}

// Define adjustment factors
Q.adjustmentFactors_spd_wurttemberg = 0.85;
Q.adjustmentFactors_aspd_wurttemberg = 0.2;
Q.adjustmentFactors_kpd_wurttemberg = 0.85;
Q.adjustmentFactors_ddp_wurttemberg = 3.0;
Q.adjustmentFactors_dbp_wurttemberg = 0.0;
Q.adjustmentFactors_csrp_wurttemberg = 2.0;
Q.adjustmentFactors_z_wurttemberg = 1.45;
Q.adjustmentFactors_dvp_wurttemberg = 1.0;
Q.adjustmentFactors_csvd_wurttemberg = 2.5;
Q.adjustmentFactors_wp_wurttemberg = 0.0;
Q.adjustmentFactors_cnblp_wurttemberg = 7.0;
Q.adjustmentFactors_kvp_wurttemberg = 0.8;
Q.adjustmentFactors_dnvp_wurttemberg = 0.6;
Q.adjustmentFactors_dnf_wurttemberg = 0.5;
Q.adjustmentFactors_nsdap_wurttemberg = 0.7;
Q.adjustmentFactors_other_wurttemberg = 0.9;

if (Q.lvp_formed || Q.nfvp_formed) {
    Q.adjustmentFactors_dvp_wurttemberg += 1.5
}

if (Q.cnblp_kvp) {
    Q.adjustmentFactors_kvp_wurttemberg += 1.5
}
if (Q.cnblp_dnvp) {
    Q.adjustmentFactors_dnvp_wurttemberg += 1.5
}



// Calculate support for each of the parties
var total_support = 0;
for (var party of Q.parties) {
    var party_support = 0;
    for (var c of Q.classes) {
        if (Q.old_demographics) {
            party_support += Q[c] * Q[c + '_' + party];
        } else {
            party_support += Q[c] * Q[c + '_' + party + '_normalized_wurttemberg'];
        }
    }
    // Apply adjustment factor
    Q[party + '_support'] = party_support * Q['adjustmentFactors_' + party + '_wurttemberg']
    total_support += Q[party + '_support'];
}

// Normalize support (fraction)
for (var party of Q.parties) {
    Q[party + '_normalized_wurttemberg'] = Q[party + '_support'] / total_support;
    Q[party + '_votes_wurttemberg'] = Math.round(Q[party + '_normalized_wurttemberg'] * 1000) / 10;
}

var new_party_sum = 0;
for (var party of Q.parties) {
    Q['old_' + party + '_wl'] = Q[party + '_wl'];
    Q[party + '_votes_wurttemberg'] = parseFloat((Q[party + '_votes_wurttemberg']).toFixed(1))
    if (Q.electoral_threshold > 0) {
        if (Q[party + '_votes_wurttemberg'] < Q.electoral_threshold || (Q.electoral_threshold > 1 && party == 'other') || Q[party + '_banned']) {
            Q[party + '_wl'] = 0;
        } else {
            Q[party + '_wl'] = Q[party + '_votes_wurttemberg'];
        }
        new_party_sum += Q[party + '_wl'];
    } else {
        Q[party + '_wl'] = Q[party + '_votes_wurttemberg'];
        if (party == 'other') Q[party + '_wl'] = Q[party + '_votes_wurttemberg'] * 0.3;
        new_party_sum += Q[party + '_wl'];
    }

    Q['change_' + party + '_votes_wurttemberg'] = Q[party + '_votes_wurttemberg'] - Q['old_' + party + '_votes_wurttemberg'];
    var sign2 = Q['change_' + party + '_votes_wurttemberg'] > 0 
                    ? '<img src="img/arrowup.png"> ' 
                    : Q['change_' + party + '_votes_wurttemberg'] < 0 
                      ? '<img src="img/arrowdown.png"> ' 
                      : '<img src="img/steady.png"> ';
    Q['votes_change_' + party + '_wurttemberg'] = sign2 + Math.abs(parseFloat(Q['change_' + party + '_votes_wurttemberg'])).toFixed(1);

    Q['old_' + party + '_votes_wurttemberg'] = parseFloat((Q[party + '_votes_wurttemberg']).toFixed(1));
}
// calculate new party sum if electoral reform
if (new_party_sum > 0) {
    for (var party of Q.parties) {
        if (Q[party + '_wl'] > 0) {
            Q[party + '_wl'] = Math.round(Q[party + '_wl']*1000/new_party_sum)/10;
        }
        Q[party + '_wl'] = parseFloat((Q[party+'_wl']).toFixed(1));
        
        Q['change_' + party + '_wl'] = Q[party + '_wl'] - Q['old_' + party + '_wl'];
        var sign = Q['change_' + party + '_wl'] > 0 
                   ? '<img src="img/arrowup.png"> ' 
                   : Q['change_' + party + '_wl'] < 0
                     ? '<img src="img/arrowdown.png"> '
                     : '<img src="img/steady.png"> ';
        Q['change_' + party + '_wl'] = sign + Math.abs(parseFloat(Q['change_' + party + '_wl'])).toFixed(1);
    }
}



if (!Q.lvp_formed && !Q.nfvp_formed) Q.weimar_coalition_wurttemberg = parseFloat((Q.spd_wl + Q.ddp_wl + Q.z_wl + Q.csrp_wl).toFixed(1));
if (Q.lvp_formed || Q.nfvp_formed) Q.weimar_coalition_wurttemberg = parseFloat((Q.spd_wl + Q.dvp_wl + Q.z_wl + Q.csrp_wl).toFixed(1));
if (Q.aspd_exist && Q.aspd_ideology == "Moderate") Q.weimar_coalition_wurttemberg = parseFloat((Q.weimar_coalition_wurttemberg + Q.aspd_wl).toFixed(1));

Q.grand_coalition_wurttemberg = parseFloat((Q.spd_wl + Q.ddp_wl + Q.z_wl + Q.dvp_wl + Q.csrp_wl).toFixed(1));
if (Q.aspd_exist && Q.aspd_ideology == "Moderate") Q.grand_coalition_wurttemberg = parseFloat((Q.grand_coalition_wurttemberg + Q.aspd_wl).toFixed(1));

Q.bourgeois_coalition_wurttemberg = parseFloat((Q.ddp_wl + Q.z_wl + Q.dvp_wl + Q.other_wl + Q.cnblp_wl + Q.kvp_wl + Q.csvd_wl + Q.csrp_wl).toFixed(1));
if (Q.aspd_exist && Q.aspd_ideology == "Moderate") Q.bourgeois_coalition_wurttemberg = parseFloat((Q.bourgeois_coalition_wurttemberg + Q.aspd_wl).toFixed(1));

if (Q.dnvp_ideology === "Radical") Q.far_right_coalition_wurttemberg = parseFloat((Q.dnvp_wl + Q.nsdap_wl).toFixed(1));
if (Q.dnf_formed) Q.far_right_coalition_wurttemberg = parseFloat((Q.dnf_wl + Q.nsdap_wl).toFixed(1));



if (Q.dvp_wurttemberg) {
    Q.was_dvp_wurttemberg = 1
}
if (Q.in_dvp_majority_wurttemberg) {
    Q.was_in_dvp_majority_wurttemberg = 1
} else if (Q.in_weimar_coalition_wurttemberg) {
    Q.was_in_weimar_coalition_wurttemberg = 1
} else if (Q.in_grand_coalition_wurttemberg) {
    Q.was_in_grand_coalition_wurttemberg = 1
} else if (Q.in_bourgeois_coalition_wurttemberg) {
    Q.was_in_bourgeois_coalition_wurttemberg = 1
} else if (Q.in_far_right_coalition_wurttemberg) {
    Q.was_in_far_right_coalition_wurttemberg = 1
}


Q.dvp_wurttemberg = 0;
Q.in_dvp_majority_wurttemberg = 0;
Q.in_weimar_coalition_wurttemberg = 0;
Q.in_grand_coalition_wurttemberg = 0;
Q.in_bourgeois_coalition_wurttemberg = 0;
Q.in_far_right_coalition_wurttemberg = 0;
!}
